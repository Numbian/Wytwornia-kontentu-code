<script src="js/pannelum.js"></script>
<link rel="stylesheet" href="css/pannelum.css">
<style>

#panorama {
        
    height: 85vh;
    }
    
    .hideOverlay{
       display:none;
    }
   
    .custom-hotspot {
        height: 70px;
        width: 70px;
        background-image: url(images/arrow3d.svg);
        background-repeat: no-repeat;
        
        filter: drop-shadow( 3px 3px 2px rgba(0, 0, 0, .7));
        
         box-shadow: 0 0 0 rgba(204,169,44, 0.4);
         animation: shadowPulsing 2s infinite;
}
    }
    .custom-hotspot:hover {
        filter: hue-rotate(10deg);
    }

.mainMargin{
    margin-top: 58px;
}
@media (min-width: 991px){
.mainMargin {
    margin-top: 72px;
}}

@-webkit-keyframes shadowPulsing {
 0% {
    filter: drop-shadow( 5px 5px 2px rgba(0, 0, 0, .9));
  }
  70% {
      filter: drop-shadow( 15px 15px 8px rgba(0, 0, 0, .7));
  }
  100% {
      filter: drop-shadow( 5px 5px 2px rgba(0, 0, 0, .9));
  }
}
@keyframes shadowPulsing {
   0% {
    filter: drop-shadow( 5px 5px 2px rgba(0, 0, 0, .9));
  }
  70% {
      filter: drop-shadow( 15px 15px 8px rgba(0, 0, 0, .7));
  }
  100% {
      filter: drop-shadow( 5px 5px 2px rgba(0, 0, 0, .9));
  }
}

@media only screen and (max-width: 800px) {
    #panorama {   
    height: 73vh;
    }
    
    .custom-hotspot {
        height: 50px;
        width: 50px;
    }
    
    
  }  
  
</style>



<div class="mainMargin">
<div id="panorama" style="position: relative; width:100%;" class="pnlm-container">
